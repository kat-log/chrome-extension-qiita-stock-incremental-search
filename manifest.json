{
  "manifest_version": 3,
  "name": "My Custom Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://qiita.com/*"],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
